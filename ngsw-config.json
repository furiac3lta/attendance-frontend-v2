{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js",
          "/icons/*.png",
          "/*.(ico|svg)"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/**/*.(png|jpg|jpeg|gif|webp|svg|ico)",
          "/**/*.(woff|woff2|ttf|otf)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api",
      "urls": [
        "https://**/*",
        "http://**/*"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 1,
        "maxAge": "0s",
        "timeout": "0s"
      }
    }
  ]
}
